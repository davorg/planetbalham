//-->

</script>
<script type="text/javascript"
src="http://pagead2.googlesyndication.com/pagead/show_ads.js">
</script>
</p>

		<!-- sidebar ends -->		
		</div>		
		
		
	<!-- content-wrap ends-->	
	</div>
		
	<!-- footer starts -->		
	<div id="footer-wrap">
		<div id="footer-columns">
	
			<div class="col3">
			</div>

			<div class="col3-center">
			</div>

			<div class="col3">
			</div>
		<!-- footer-columns ends -->
		</div>	
	
		<div id="footer-bottom">		
    <p>By <a href="mailto:[% cfg.author.email %]">[% cfg.author.name | html %]</a> | Built at [% feed.modified.strftime('%A %d %B %H:%M:%S %Z') %] | Powered by <a href="http://perlanet.mag-sol.com/">Perlanet</a><br />
    Design by: <a href="http://www.styleshout.com/">styleshout</a> | Original <a href="http://www.flickr.com/photos/rjw1/673880462/">Photo</a> by rjw1</p>
			
		</div>	

<!-- footer ends-->
</div>
<!-- wrap ends here -->
</div>
